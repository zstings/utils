{
  "name": "@zstings/utils",
  "uname": "utils",
  "version": "0.5.4",
  "license": "MIT",
  "author": "@zstings",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/utils.umd.js",
  "module": "./dist/utils.es.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.umd.js",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && tsc-alias",
    "doc": "typedoc --options ./typedoc.json",
    "docs:json": "typedoc --json create_docs_md/data.json && node ./create_docs_md/index.mjs",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docsvite",
    "docs:build": "vitepress build docsvite",
    "docs:serve": "vitepress serve docsvite"
  },
  "keywords": [
    "tings",
    "utils",
    "jsutils",
    "tsutils",
    "js工具函数",
    "ts工具函数"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.7.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitepress": "1.0.0-alpha.30"
  },
  "homepage": "https://github.com/zstings/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zstings/utils.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}