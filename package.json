{
  "name": "@zstings/utils",
  "uname": "utils",
  "version": "0.9.2",
  "license": "MIT",
  "author": "@zstings",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/utils.umd.js",
  "module": "./dist/utils.es.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.umd.js",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint --fix --ext .md",
    "dev": "vite",
    "build": "vite build && tsc && tsc-alias && node run.js && vitepress build docsvite",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docsvite",
    "docs:build": "vitepress build docsvite",
    "docs:serve": "vitepress serve docsvite",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "tings",
    "utils",
    "jsutils",
    "tsutils",
    "js工具函数",
    "ts工具函数"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^15.7.4",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitepress": "1.4.3",
    "vitest": "^2.1.4"
  },
  "homepage": "https://github.com/zstings/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zstings/utils.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
