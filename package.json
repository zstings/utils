{
  "name": "@zstings/utils",
  "uname": "utils",
  "version": "0.9.2",
  "license": "MIT",
  "author": "@zstings",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/utils.umd.js",
  "module": "./dist/utils.es.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.umd.js",
      "default": "./dist/utils.es.js"
    }
  },
  "scripts": {
    "lint": "eslint --fix --ext .md",
    "dev": "vite",
    "build": "vite build && tsc && tsc-alias && node run.js && vitepress build vitepress && node deploy.js",
    "preview": "vite preview",
    "docs:dev": "vitepress dev vitepress",
    "docs:build": "vitepress build vitepress",
    "docs:serve": "vitepress serve vitepress",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "tings",
    "utils",
    "jsutils",
    "tsutils",
    "js工具函数",
    "ts工具函数"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.9",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.1",
    "prettier": "^3.6.2",
    "shelljs": "^0.10.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitepress": "1.6.4",
    "vitest": "^4.0.9",
    "comment-parser": "^1.4.1"
  },
  "homepage": "https://github.com/zstings/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zstings/utils.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
