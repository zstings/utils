{
  "name": "tings-utils",
  "version": "0.3.0",
  "license": "MIT",
  "author": "tings",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/tings-utils.umd.js",
  "module": "./dist/tings-utils.es.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tings-utils.es.js",
      "require": "./dist/tings-utils.umd.js",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && tsc-alias",
    "doc": "typedoc --options ./typedoc.json",
    "docs:json": "typedoc --json create_docs_md/data.json && node ./create_docs_md/index.mjs",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docsvite",
    "docs:build": "vitepress build docsvite",
    "docs:serve": "vitepress serve docsvite"
  },
  "keywords": [
    "tings",
    "utils",
    "js-utils",
    "jsutils",
    "ts-utils",
    "tsutils",
    "ts-tools",
    "tstools",
    "js-tools",
    "jstools",
    "工具函数",
    "js工具",
    "ts工具"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.7.0",
    "typedoc": "^0.23.11",
    "typescript": "^4.8.3",
    "vite": "^3.0.9",
    "vitepress": "1.0.0-alpha.12"
  },
  "homepage": "https://github.com/qinzhongting/tings-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qinzhongting/tings-utils.git"
  }
}